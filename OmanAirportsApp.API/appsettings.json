{
  "ConnectionStrings": {
    "OmanAirportDBConnection": "Server=localhost\\SQLEXPRESS;Database=omanairportdb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinumumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInteval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}